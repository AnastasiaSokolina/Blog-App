{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
      ],
    "parser": "babel-eslint",  
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "prettier/prettier": "error"
    }
  }